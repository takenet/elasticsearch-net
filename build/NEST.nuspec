<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2012/06/nuspec.xsd">
	<metadata>
		<id>Take.NEST</id>
		<version>$version$</version>
		<title>Unofficial (by Take) NEST - Elasticsearch .NET high level client</title>
		<authors>Take, Elastic and contributors</authors>
		<owners>takenet</owners>
		<requireLicenseAcceptance>false</requireLicenseAcceptance>
		<license type="file">license.txt</license>
		<projectUrl>https://github.com/takenet/elasticsearch-net</projectUrl>
		<description>Unofficial (by Take) strongly typed interface to Elasticsearch. Fluent and classic object initializer mappings of requests and responses. Uses and exposes Elasticsearch.Net</description>
		<summary>Unofficial (by Take) strongly typed interface to Elasticsearch. Fluent and classic object initializer mappings of requests and responses. Uses and exposes Elasticsearch.Net</summary>
		<tags>elasticsearch,elastic,search,lucene,nest</tags>
		<repository type="git" url="https://github.com/takenet/elasticsearch-net.git" branch="$branch$" commit="$commit$" />
		<dependencies>
			<group targetFramework="net461">
				<dependency id="Take.Elasticsearch.Net" version="[$version$, 7.0.0)" />
			</group>
			<group targetFramework="netstandard2.0">
				<dependency id="Take.Elasticsearch.Net" version="[$version$, 7.0.0)" />
			</group>
		</dependencies>
	</metadata>
	<files>
		<file src="..\license.txt" target="" />
		<file src="nuget-icon.png" target="images\" />
	
		<file src="output\Nest\net461\Nest.dll" target="lib\net461"/>
		<file src="output\Nest\net461\Nest.pdb" target="lib\net461"/>
		<file src="output\Nest\net461\Nest.xml" target="lib\net461"/>

		<file src="output\Nest\netstandard2.0\Nest.dll" target="lib\netstandard2.0"/>
		<file src="output\Nest\netstandard2.0\Nest.pdb" target="lib\netstandard2.0"/>
		<file src="output\Nest\netstandard2.0\Nest.xml" target="lib\netstandard2.0"/>
	</files>
</package>