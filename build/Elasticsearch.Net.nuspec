<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2012/06/nuspec.xsd">
	<metadata>
		<id>Take.Elasticsearch.Net</id>
		<version>$version$</version>
		<title>Unofficial (by Take) Elasticsearch.Net - Elasticsearch .NET low level client</title>
		<authors>Take, Elastic and contributors</authors>
		<owners>takenet</owners>
		<license type="file">license.txt</license>
		<projectUrl>https://github.com/takenet/elasticsearch-net</projectUrl>
		<requireLicenseAcceptance>false</requireLicenseAcceptance>
		<description>Elasticsearch.Net</description>
		<summary>Exposes all the elasticsearch API endpoints but leaves you in control of building the request and response bodies. Comes with built in cluster failover/connection pooling support.</summary>
		<tags>elasticsearch,elastic,search,lucene,nest</tags>
		<repository type="git" url="https://github.com/takenet/elasticsearch-net.git" branch="$branch$" commit="$commit$" />
		<dependencies>
			<group targetFramework="netstandard2.0" />
			<group targetFramework="net461" />
		</dependencies>
	</metadata>
	<files>
		<file src="..\license.txt" target="" />
		<file src="nuget-icon.png" target="images\" />
	
		<file src="output\Elasticsearch.Net\netstandard2.0\Elasticsearch.Net.dll" target="lib\netstandard2.0"/>
		<file src="output\Elasticsearch.Net\netstandard2.0\Elasticsearch.Net.pdb" target="lib\netstandard2.0"/>
		<file src="output\Elasticsearch.Net\netstandard2.0\Elasticsearch.Net.xml" target="lib\netstandard2.0"/>

		<file src="output\Elasticsearch.Net\net461\Elasticsearch.Net.dll" target="lib\net461"/>
		<file src="output\Elasticsearch.Net\net461\Elasticsearch.Net.pdb" target="lib\net461"/>
		<file src="output\Elasticsearch.Net\net461\Elasticsearch.Net.xml" target="lib\net461"/>
	</files>
</package>
